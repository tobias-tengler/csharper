{
  "name": "csharper",
  "displayName": "CSharper",
  "version": "0.0.4",
  "description": "Easily create templated C# files",
  "license": "MIT",
  "author": {
    "name": "Tobias Tengler",
    "url": "https://github.com/tobias-tengler"
  },
  "publisher": "tobiastengler",
  "repository": "https://github.com/tobias-tengler/csharper",
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "activationEvents": [
    "onLanguage:csharp",
    "onCommand:csharper.newFile"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "csharper.newFile",
        "title": "New C# File"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "group": "navigation@-1",
          "command": "csharper.newFile"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.40.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "typescript": "^3.7.2",
    "tslint": "^5.20.0",
    "vscode-test": "^1.2.2"
  }
}
