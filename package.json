{
  "name": "csharper",
  "displayName": "CSharper",
  "version": "0.2.1",
  "description": "Easily create templated C# files",
  "license": "MIT",
  "author": {
    "name": "Tobias Tengler",
    "url": "https://github.com/tobias-tengler"
  },
  "publisher": "tobiastengler",
  "repository": "https://github.com/tobias-tengler/csharper",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "activationEvents": [
    "onLanguage:csharp",
    "onCommand:csharper.newFile"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "csharper.newFile",
        "title": "New C# File"
      }
    ],
    "keybindings": [
      {
        "command": "csharper.newFile",
        "key": "ctrl+j ctrl+j"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "group": "navigation@-1",
          "command": "csharper.newFile"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.34",
    "@types/vscode": "^1.40.0",
    "glob": "^7.1.5",
    "mocha": "^8.3.2",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3",
    "vscode-test": "^1.2.2"
  }
}
